{
  "name": "react-base",
  "version": "1.0.0",
  "description": "Custom boilerplate for react development.",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "webpack-dev-server --open",
    "clean": "rimraf dist node_modules"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willfull-git/react-base.git"
  },
  "keywords": [
    "react",
    "boilerplate"
  ],
  "author": "willfull",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/willfull-git/react-base/issues"
  },
  "homepage": "https://github.com/willfull-git/react-base#readme",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^4.3.0",
    "rimraf": "^3.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.1"
  }
}
